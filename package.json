{
  "name": "@dr-kobros/react-broilerplate-project",
  "version": "1.2.0",
  "description": "An example project utilizing broilerplate",
  "main": "src/client.jsx",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:webpack": "NODE_ENV=production webpack --progress -p",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint src --ext js --ext jsx",
    "babel-node": "babel-node",
    "start": "NODE_ENV=development nodemon --exec npm run babel-node -w src/server.js -- src/server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --require ignore-styles",
    "flow": "flow",
    "serve": "NODE_ENV=production ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node src/server.js",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-broilerplate-project"
  },
  "keywords": [
    "team"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@dr-kobros/react-broilerplate": "^0.15.1",
    "@dr-kobros/react-broilerplate-server-express": "^0.8.0",
    "axios": "^0.14.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "node-uuid": "^1.4.7",
    "normalize.css": "^4.2.0",
    "react-fa": "^4.1.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-inspector": "^1.1.0",
    "redux-promise-middleware": "^4.0.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.20.1",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "empty": "^0.10.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "flow-bin": "^0.32.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "react-addons-test-utils": "^15.3.1"
  }
}
