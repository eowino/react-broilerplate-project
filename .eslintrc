{
  "extends": "airbnb",

  rules: {
    "new-cap": [2, {
      "capIsNewExceptions": [
        "List",
        "Map",
        "Seq",
        "Set",
        "Range",
        "Immutable.Map",
        "Immutable.Set",
        "Immutable.List",
        "Immutable.Seq",
        "Immutable.Range"
      ]
    }],

    "react/jsx-no-bind": [1],
    "import/no-extraneous-dependencies": "off"
  },

  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  }

}
